{
  "manifest_version": 2,
  "name": "Remove frame around jira sprint board",
  "version": "1.0",
  "description": "The frame breaks 'full page screenshot' functionality",
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "camera.png",
    "default_title": "Remove the frame for screenshot!"
  },
  "permissions": [
    "activeTab"
  ]
}
